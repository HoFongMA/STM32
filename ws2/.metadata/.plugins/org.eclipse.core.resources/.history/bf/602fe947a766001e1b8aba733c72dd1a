/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{
  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_COMP1_Init();
  MX_I2C1_Init();
  MX_IRTIM_Init();
  MX_SPI1_Init();
  MX_TIM1_Init();
  MX_TIM15_Init();
  MX_TIM16_Init();
  MX_USART1_UART_Init();
  MX_RTC_Init();
// [CKL worklog 22-W12-07] : Activate a 8192ms Watchdog (code generated by STM32Cube)  
  MX_IWDG_Init();
// [CKL end]  
  /* USER CODE BEGIN 2 */
  //HAL_NVIC_SetPriority(FLASH_IRQn, 0, 0);
  //HAL_NVIC_EnableIRQ(FLASH_IRQn);
  //HAL_FLASH_Unlock();
  CLK.init(); //init clock
  CLK.tick(HAL_GetTick()); //get initial time
  //IND.init(&CLK);
  DSPLY.init(&CLK, &hi2c1);
  THRM.init(); //init thermometer
  BM.init(hspi1, &CLK, &THRM, &DSPLY, hrtc); //init balance master
  INTF.init(&CLK, &BM, &huart1); //init interface
  HAL_UART_Receive_IT(&huart1, UART1_rxBuffer, 1); //init nonblocking UART rx

  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_6, GPIO_PIN_RESET);
  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_7, GPIO_PIN_RESET);
  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_8, GPIO_PIN_RESET);

  // [FF 2023.10.09]
  char			uart_msg[50];
  uint8_t		uart_len;
  uart_len = sprintf( uart_msg, "\r\n\n Start of the T150 BMS in 3 sec.\r\n\n " );
  HAL_UART_Transmit(&huart1, (uint8_t*)uart_msg, uart_len, 100);

  if( __HAL_PWR_GET_FLAG( PWR_FLAG_SB ) != RESET ){
	  uart_len = sprintf( uart_msg, "\r\n\n Restart from standby mode.\r\n\n " );
	  HAL_UART_Transmit(&huart1, (uint8_t*)uart_msg, uart_len, 100);
	  __HAL_PWR_CLEAR_FLAG( PWR_FLAG_SB );
	  HAL_PWR_DisableWakeUpPin( PWR_WAKEUP_PIN1 );
	  HAL_RTCEx_DeactivateWakeUpTimer( &hrtc );
  }
  FLASH_OBProgramInitTypeDef OB;
  HAL_FLASHEx_OBGetConfig( &OB );
  // OB_USER_IWDG_STDBY#
  uint32_t _a = OB.USERConfig;
  _a &= FLASH_OPTR_IWDG_STDBY;
//  if( _a &= OB_USER_IWDG_STDBY ){ 00040000 watchdog counter freeze *** FLASH_OPTR_IWDG_STDBY
  if( _a > 0 ){
	  uart_len = sprintf( uart_msg, "\r\n\n Watchdog pin high\r\n\n " );
	  HAL_UART_Transmit(&huart1, (uint8_t*)uart_msg, uart_len, 100);
	  while( HAL_FLASH_OB_Unlock( ) != HAL_OK ){
		  uart_len = sprintf( uart_msg, "\r\n\n Flash OB not able to unlock \r\n\n " );
		  HAL_UART_Transmit(&huart1, (uint8_t*)uart_msg, uart_len, 100);
		  HAL_Delay(1000);
	  }
	  _a = OB.USERConfig;

	  _a &= (FLASH_OPTR_IWDG_STDBY^0xFFFFFFFF);

	  OB.USERConfig = _a;
	  HAL_FLASHEx_OBProgram( &OB );

	  HAL_FLASH_OB_Lock();
  }

  HAL_Delay( 3000 ); // delay to read the debug msg
  // [FF End]

  //TEST CODE FOR FLASH STORAGE//
//  BM.read_battery_cycles(BM.address);
//  BM.write_battery_cycles(0x08100000, (uint64_t) 0xFFFFFFFFFFFFFFFF); //This completely erases the flash memory
//  BM.read_battery_cycles(BM.address);
  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
  {
    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */

	  /////////////////////////////////////////////////////////////////////////////////
	  ///FIRMWARE VERSION NEEDS TO BE UPDATED IN INTERFACE::INIT IF CHANGES ARE MADE///
	  /////////////////////////////////////////////////////////////////////////////////

	  CLK.tick(HAL_GetTick()); //update timers
	  INTF.input(); //get user inputs
	  BM.work(); //do the thing
	  DSPLY.work();
	  INTF.output();
	  //if(BM.startup_phase != 1) INTF.output(); //only print data to BMS after startup period, LTC chips need to stabilise
    /* USER CODE BEGIN 3 */
  // [CKL worklog 22-W12-07] : Activate a 32s Watchdog (code generated by STM32Cube)  
    HAL_IWDG_Refresh(&hiwdg);   // Refresh the watchdog timer
  // [CKL end]  
  }
  /* USER CODE END 3 */
}

/**
  * @brief System Clock Configuration
  * @retval None
  */
void SystemClock_Config(void)
{
  RCC_OscInitTypeDef RCC_OscInitStruct = {0};
  RCC_ClkInitTypeDef RCC_ClkInitStruct = {0};
  RCC_PeriphCLKInitTypeDef PeriphClkInit = {0};

  /** Configure LSE Drive Capability
  */
  HAL_PWR_EnableBkUpAccess();
  __HAL_RCC_LSEDRIVE_CONFIG(RCC_LSEDRIVE_LOW);
  /** Configure the main internal regulator output voltage
  */
  if (HAL_PWREx_ControlVoltageScaling(PWR_REGULATOR_VOLTAGE_SCALE1) != HAL_OK)
  {
    Error_Handler();
  }
  /** Initializes the RCC Oscillators according to the specified parameters
  * in the RCC_OscInitTypeDef structure.
  */
  RCC_OscInitStruct.OscillatorType = RCC_OSCILLATORTYPE_MSI;
  RCC_OscInitStruct.MSIState = RCC_MSI_ON;
  RCC_OscInitStruct.MSICalibrationValue = 0;
  RCC_OscInitStruct.MSIClockRange = RCC_MSIRANGE_6;
  RCC_OscInitStruct.PLL.PLLState = RCC_PLL_ON;
  RCC_OscInitStruct.PLL.PLLSource = RCC_PLLSOURCE_MSI;
  RCC_OscInitStruct.PLL.PLLM = 1;
  RCC_OscInitStruct.PLL.PLLN = 40;
  RCC_OscInitStruct.PLL.PLLQ = RCC_PLLQ_DIV2;
  RCC_OscInitStruct.PLL.PLLR = RCC_PLLR_DIV2;

  if (HAL_RCC_OscConfig(&RCC_OscInitStruct) != HAL_OK)
  {
    Error_Handler();
  }
  /** Initializes the CPU, AHB and APB buses clocks
  */
  RCC_ClkInitStruct.ClockType = RCC_CLOCKTYPE_HCLK|RCC_CLOCKTYPE_SYSCLK
                              |RCC_CLOCKTYPE_PCLK1|RCC_CLOCKTYPE_PCLK2;
  RCC_ClkInitStruct.SYSCLKSource = RCC_SYSCLKSOURCE_PLLCLK;
  RCC_ClkInitStruct.AHBCLKDivider = RCC_SYSCLK_DIV1;
  RCC_ClkInitStruct.APB1CLKDivider = RCC_HCLK_DIV8;
  RCC_ClkInitStruct.APB2CLKDivider = RCC_HCLK_DIV1;

  if (HAL_RCC_ClockConfig(&RCC_ClkInitStruct, FLASH_LATENCY_4) != HAL_OK)
  {
    Error_Handler();
  }
  PeriphClkInit.PeriphClockSelection = RCC_PERIPHCLK_USART1|RCC_PERIPHCLK_I2C1;
  PeriphClkInit.Usart1ClockSelection = RCC_USART1CLKSOURCE_SYSCLK;
  PeriphClkInit.I2c1ClockSelection = RCC_I2C1CLKSOURCE_PCLK1;
  if (HAL_RCCEx_PeriphCLKConfig(&PeriphClkInit) != HAL_OK)
  {
    Error_Handler();
  }
  /** Enable MSI Auto calibration
  */
  HAL_RCCEx_EnableMSIPLLMode();
}

